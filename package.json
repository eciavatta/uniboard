{
  "name": "uniboard",
  "version": "0.1.0",
  "description": "Web application that shows the free classrooms and provides support to the students of the Cesena Campus",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server.js --exec babel-node --watch public --watch src --watch build ",
    "build": "rm -rf dist/* && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.babel.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.babel.js",
    "start": "cross-env NODE_ENV=production node dist/server-bundle.js",
    "migrate": "node src/migrate.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eciavatta/uniboard.git"
  },
  "keywords": [
    "unibo",
    "cesena",
    "campus",
    "lessons"
  ],
  "author": "Emiliano Ciavatta, Luca Tremamunno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eciavatta/uniboard/issues"
  },
  "homepage": "https://github.com/eciavatta/uniboard#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.3",
    "react-transition-group": "^4.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "and": "0.0.3",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "http-errors": "^1.7.3",
    "mongoose": "^5.6.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-router-dom": "^5.0.1",
    "react-svg-pan-zoom": "^3.1.0"
  }
}
